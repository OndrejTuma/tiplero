<!doctype html>
<html lang="cs">
<head>
    {% include "../fragments/head.twig" %}
</head>
<body>
{% block header %}
    <!-- SVG MAP -->
    <span style="display: none">
        {#
        {% include "../../build/svg/svg-symbols.svg" %}
        #}
    </span>
    <!-- HEADER -->
    {% if user.isLogged %}
        {% include "../fragments/header_logged.twig" %}
    {% else %}
        {% include "../fragments/header.twig" %}
    {% endif %}
{% endblock %}

<!-- MAIN -->
<main>
    {% if breadcrumbs is defined %}
        <div class="container inner--narrow">
            {% include "../components/breadcrumbs.twig" with {breadcrumbs: breadcrumbs} only %}
        </div>
    {% endif %}
    {% block content %}

    {% endblock %}
</main>

{% block footer %}
    <!-- FOOTER -->
    {% include "../fragments/footer.twig" %}
{% endblock %}

<!-- SCRIPTS -->
{% include "../fragments/scripts.twig" %}
</body>
</html>